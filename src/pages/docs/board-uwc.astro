import Layout from '../../layouts/Layout.astro';

import * as post from '../../assets/foundation/docs/incorporation/board-uwc.md';
---

<Layout title="Board Unanimous Written Consent — Open Source Endowment" description="Board of Directors Unanimous Written Consent of the Open Source Endowment Foundation.">
    <script type="application/ld+json" set:html={JSON.stringify({
        "@context": "https://schema.org",
        "@type": "BreadcrumbList",
        "itemListElement": [
            { "@type": "ListItem", "position": 1, "name": "Home", "item": "https://endowment.dev/" },
            { "@type": "ListItem", "position": 2, "name": "Docs", "item": "https://endowment.dev/docs" },
            { "@type": "ListItem", "position": 3, "name": "Board Unanimous Written Consent" }
        ]
    })} />
    <section>
        <div class="container">
            <article>
                <section class="pagesec">
                    <div class="insec insec--xpad doc-content">
                        <Fragment set:html={post.compiledContent()}/>
                    </div>
                </section>
            </article>
        </div>
    </section>
</layout>
